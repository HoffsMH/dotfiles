@XF86MonBrightnessDown
  light -U 5

@XF86MonBrightnessUp
  light -A 5

@XF86AudioLowerVolume
  pulsemixer --change-volume -3

@XF86AudioRaiseVolume
  pulsemixer --change-volume +3

@XF86KbdBrightnessUp
  light -As "sysfs/leds/smc::kbd_backlight" 3

@XF86KbdBrightnessDown
  light -Us "sysfs/leds/smc::kbd_backlight" 3

control + shift + 4
  flameshot gui